[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/4bh1
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/4bh1
	label-Verified = -1..+1 group user/4bh1
	push = group user/4bh1
	read = group user/4bh1
	rebase = group user/4bh1
	removeReviewer = group user/4bh1
	submit = group user/4bh1
[access "refs/heads/*"]
	create = group user/4bh1
	push = +force group user/4bh1
	pushMerge = group user/4bh1
	read = group user/4bh1
[access "refs/tags/*"]
	push = group user/4bh1
	push = +force group user/4bh1
